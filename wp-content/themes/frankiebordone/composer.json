{
  "name": "fbordone/frankiebordone",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "Custom WordPress site with headless CMS and utilization of React library",
  "homepage": "https://frankiebordone.com",
  "authors": [
    {
      "name": "Frankie Bordone",
      "email": "bordone.francesco@gmail.com",
      "homepage": "https://frankiebordone.com"
    }
  ],
  "keywords": ["developer", "full stack", "portfolio", "React"],
  "support": {
    "issues": "https://github.com/fbordone/frankiebordone/issues"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=7.1",
    "composer/installers": "~1.0",
    "illuminate/support": "5.6.*",
    "roots/sage-lib": "~9.0.9",
    "soberwp/controller": "~2.1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0",
    "roots/sage-installer": "~1.6"
  },
  "scripts": {
    "test": ["phpcs"],
    "post-create-project-cmd": [
      "Roots\\Sage\\Installer\\ComposerScript::postCreateProject"
    ]
  }
}
